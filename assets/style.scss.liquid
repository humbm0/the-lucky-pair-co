/*MEMBERSHIPS FORM STYLING AND ANIMATION*/

.modal-header {
  height: 45px;
  background: white;
  border-bottom: 1px solid #ccc;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.modal-header span {
  display: block;
  height: 12px;
  width: 12px;
  margin: 5px;
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.2);
}
.modal-header span.is-active {
  background: rgba(0, 0, 0, 0.4);
  background: #FF8F00;
}
.modal-wrap {
  top: 0;
  position: relative;
  min-height: 800px;
  margin: 50px auto;
  -webkit-transition: -webkit-transform 300ms ease-in-out;
  transition: -webkit-transform 300ms ease-in-out;
  transition: transform 300ms ease-in-out;
  transition: transform 300ms ease-in-out, -webkit-transform 300ms ease-in-out;
}
.modal-bodies {
  position: relative;
}

.modal-body {
  background: white;
  padding: 40px 100px;
  margin-bottom: 50px;
  position: absolute;
  top: 0;
  display: none;
  box-sizing: border-box;
  width: 100%;
  -webkit-transform-origin: top left;
          transform-origin: top left;
}
.modal-body.is-showing {
  display: block;
}
.modal-body input[type=radio]{
    display:none;
}
.modal-body .button-group{
  display: block;
}
.modal-body label.tab{
  display: inline-block;
  display: table-cell;
  padding: 10px 0;
  cursor: pointer;
}
.modal-body input[type=radio]:checked + label.tab{
  background: $primary-color;
}
.modal-body label.tab:hover{
  background: $primary-color;
}
.modal-body .tabs{
  border: 1px solid $primary-color;
  display: table;
  padding: 0;
}
.modal-body input[type=radio]:checked + .card-radio{
    border: 3px solid $primary-color;
}
.modal-body .card-radio:hover{
    border: 3px solid $primary-color;
}
.modal-body .card-radio{
  cursor: pointer;
}

.animate-out {
  -webkit-animation: out 600ms ease-in forwards;
          animation: out 600ms ease-in forwards;
}

.animate-in {
  -webkit-animation: in 500ms ease-out forwards;
          animation: in 500ms ease-out forwards;
  display: block;
}

/*.animate-up {
  -webkit-transform: translateY(-500px) rotate(30deg);
          transform: translateY(-500px) rotate(30deg);
}*/

@-webkit-keyframes out {
  0% {
    transform: translateX(0);
    -webkit-transform: translateX(0);
  }
  100% {
    transform: translateX(-100vw);
    -webkit-transform: translateX(-100vw);
  }
}

@keyframes out {
  0% {
    transform: translateX(0);
    -webkit-transform: translateX(0);
  }
  100% {
    transform: translateX(-100vw);
    -webkit-transform: translateX(-100vw);
  }
}
@-webkit-keyframes in {
  0% {
    transform: translateX(100vw);
    -webkit-transform: translateX(100vw);
  }
  100% {
    transform: translateX(0);
    -webkit-transform: translateX(0);
  }
}
@keyframes in {
  0% {
    transform: translateX(100vw);
    -webkit-transform: translateX(100vw);
  }
  100% {
    transform: translateX(0);
    -webkit-transform: translateX(0);
  }
}

