/* ==========================================================================
   Most theme settings should be stored in Sassy CSS variables here, for reuse.
   ========================================================================== */
/*!theme settings */
$site-max-width:          60 * {{ settings.base_font_size }};
$breakpoint-small-screen: 30 * {{ settings.base_font_size }};
$gutter:                  1 *  {{ settings.base_font_size }};
$background-color:        {{ settings.background_color }};
/*$text-color:              {{ settings.text_color }};*/
$text-color:              #4D4D4D;
$primary-color:           #C8F8F6;
/*$link-color:              {{ settings.link_color }};*/
$link-color:              #333C4E;
$base-font-family:        {{ settings.base_font_family }};
$base-font-size:          {{ settings.base_font_size }};
$heading-font-family:     $base-font-family;
$heading-font-weight:     600;

/* Table of content */
/* 1.  Base styles
   2.  Forms
   3.  Tables
   4.  Layout
   5.  Header
   6.  Navigation
   7.  Tags
   8.  Social and payment methods
   9.  Blog
   10. Product and collection pages
   11. Search form and search results
   12. Footer
*/

/* =====================
   1. Base styles
   ===================== */

html, body{
  background-color: $background-color;
  font-size: $base-font-size;
  line-height: 1.6;
  font-family: $base-font-family;
  letter-spacing: 1.5px;
  font-weight: 300;
  color: $text-color;
}

.paragraph-margins { margin: 0 0 .75 * $gutter; }
.grid-margins      { margin: 0 0 $gutter;       }
.section-margins   { margin: 0 0 2 * $gutter;   }

h1, h2, h3, h4, h5, h6 {
  font-family: $heading-font-family;
  font-weight: $heading-font-weight;
  text-rendering: optimizeLegibility; /* to enable ligatures and kerning */
  @extend .paragraph-margins;
}

p{
  letter-spacing: 1.5px;
}
button:focus{
  outline: 0 !important;
}

/* http://modularscale.com/scale/?px1=16&px2=32&ra1=1.667&ra2=0 */

.tera {
  font-size: 5.558 * $base-font-size;
  line-height: 1;
}

.giga {
  font-size: 4.632 * $base-font-size;
  line-height: 1;
}

.mega {
  font-size: 3.334 * $base-font-size;
  line-height: 1;
}

.alpha, h1 {
  font-size: 2.779 * $base-font-size;
  line-height: 1.1;
}

.beta, h2 {
  font-size: 2 * $base-font-size;
  line-height: 1.1;
}

.gamma, h3 {
  font-size: 1.667 * $base-font-size;
  line-height: 1.2;
}

.delta, h4 {
  font-size: 1.2 * $base-font-size;
  line-height: 1.4;
}

.epsilon, h5 {
  font-size: 1 * $base-font-size;
  line-height: 1.6;
}

.zeta, h6 {
  font-size: 0.72 * $base-font-size;
  line-height: 1.6;
}

h1{
  font-size: 48px;
  letter-spacing: 14px;
  font-weight: 300;
  text-transform: uppercase;
}
h2{
  font-size: 36px;
  letter-spacing: 7.71px;
  font-weight: 400;
  line-height: 1.3;
  text-transform: uppercase;
}
h3{
  font-size: 24px;
  letter-spacing: 7.25px;
  line-height: 1.4;
  text-transform: uppercase;
}
h5{
  font-size: 18px;
  letter-spacing: 3.86px;
  font-weight: 400;
}
a{
  text-transform: uppercase;
  color: $text-color;
  border-bottom: 3px solid $primary-color;
  font-weight: 600;
  letter-spacing: 2.33px;
  padding-bottom: 5px;
  transition: all 0.3s ease;
  cursor: pointer;
}
a:hover{
  text-decoration: none;
  color: $primary-color;
}
.is-light{
  color: #fff;
}
.is-dark{
  color: $text-color;
}
.acent{
  font-family: AppleMyungjo;
  color: rgba(32,32,32,0.40);
}
.primary-btn{
  margin: auto;
  display: block;
  background-color: $primary-color;
  padding: 10px 60px;
  text-transform: uppercase;
  color: #212121;
  font-size: 14px;
  letter-spacing: 2.79px;
  line-height: 33px;
  border: none;
  border-radius: 0;
  transition: all 0.3s ease-out;
  font-weight: 400;
}
.primary-btn:hover{
  background-color: #A0F1EE;
  letter-spacing: 3.7px;
}
.secondary-btn{
  margin: auto;
  display: block;
  background: none;
  padding: 10px 60px;
  text-transform: uppercase;
  color: #212121;
  font-size: 14px;
  letter-spacing: 2.79px;
  line-height: 33px;
  border: none;
  border-radius: 0;
  transition: all 0.3s ease-out;
  font-weight: 400;
}
.secondary-btn:hover{
  background: inherit;
  letter-spacing: 3.7px;
}

#main{
  width: 100%;
  overflow: hidden;
}

/* Styling all pages main heading in one fell swoop. */
#main > h1, #main .title, .product-details h1 {
  @extend .beta;
}

/* Responsive images and videos. */
img, iframe { max-width: 100%; }

.quiet { color: lighten( $text-color, 10% ); }
.hint { color: lighten( $text-color, 15% ); }
i, em { font-style: italic; }
b, strong { font-weight: 600; }
.small-print { @extend .zeta; }

